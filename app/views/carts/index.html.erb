<div class="w-full">
  <p class="text-green-600 px-2 rounded bg-green-100 inline-block mb-3"><%= notice %></p>

  <div id="carts">
    <% @carts.each do |cart| %>
      <%= render cart %>
      <p>
        <%= link_to "Show this cart", cart %>
      </p>
    <% end %>
  </div>

  <%= link_to "New cart", new_cart_path %>
</div>